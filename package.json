{
  "name": "intergram",
  "version": "0.0.1",
  "description": "A live chat widget linked to your telegram messenger",
  "engines": {
    "node": "6.9.1",
    "npm": "3.10.8"
  },
  "scripts": {
    "start": "node server.js",
    "postinstall": "npm run build",
    "dev": "webpack-dev-server",
    "build": "shx mkdir -p dist && shx cp *.html dist && shx cp -r ./media dist && webpack",
    "test": "npm run -s lint",
    "lint": "eslint src"
  },
  "authors": [
    "Ido Cohen (http://github.com/idoco)"
  ],
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "css-hot-loader": "^1.4.4",
    "css-loader": "^3.2.1",
    "style-loader": "^1.0.1",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-react-jsx": "^6.4.0",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "dateformat": "^2.0.0",
    "eslint": "^3.2.2",
    "express": "4.10.2",
    "preact": "^7.1.0",
    "request": "^2.79.0",
    "shx": "^0.2.2",
    "socket.io": "^1.7.3",
    "socket.io-client": "^1.7.3",
    "store": "^1.3.20"
  }
}
