{
  "name": "intergram",
  "version": "0.0.2",
  "description": "A live chat widget linked to your telegram messenger",
  "engines": {
    "node": "6.9.1",
    "npm": "3.10.8"
  },
  "scripts": {
    "start": "node server.js",
    "postinstall": "npm run build",
    "dev": "webpack-dev-server",
    "build": "shx mkdir -p dist && shx cp *.html dist && shx cp -r ./media dist && webpack",
    "test": "npm run -s lint",
    "lint": "eslint src"
  },
  "authors": [
    "Ido Cohen (http://github.com/idoco)"
  ],
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "css-loader": "^3.2.1",
    "style-loader": "^1.0.1",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "babel-eslint": "^10.0.3",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dateformat": "^3.0.3",
    "eslint": "^6.7.2",
    "express": "^4.17.1",
    "preact": "^7.2.1",
    "request": "^2.88.0",
    "shx": "^0.3.2",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "store": "^1.3.20",
    "webpack": "^4.41.2"
  }
}
